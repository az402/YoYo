{
  "name": "sbox",
  "description": "",
  "version": "0.0.1",
  "scripts": {
    "deploy": "npm run build && cp -R ./public/* /var/www/html",
    "build": "webpack --config ./config/webpack.config.coffee",
    "prebuild": "npm run vendor",
    "vendor": "webpack --config ./config/vendor.webpack.config.js",
    "template": "consul-template -consul consul1.dev.2nd.io:8500 -template config/config.js.ctmpl:config/config.js -once",
    "develop": "webpack-dev-server --config config/webpack.local.config.coffee",
    "watch": "webpack -w --config ./config/webpack.config.coffee",
    "test": "mocha --ui bdd --compilers js:babel-core/register --require babel-polyfill --recursive --reporter spec --check-leaks test",
    "h": "cd public/dist; ls -l >> .history && cat .history"
  },
  "devDependencies": {
    "ajax-promise": "^0.8.1",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "bootstrap": "^3.3.6",
    "cjsx-loader": "^2.1.0",
    "classnames": "^2.2.3",
    "coffee-script": "^1.10.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-mocha": "^2.2.0",
    "eslint-plugin-react": "^5.1.1",
    "expect.js": "^0.3.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "live-server": "^0.9.2",
    "lodash": "^4.11.1",
    "node-fetch": "^1.3.3",
    "react": "^0.14.8",
    "react-dom": "^0.14.7",
    "react-modal": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.2",
    "react-select": "1.0.0-beta13",
    "redux": "^3.3.1",
    "redux-immutable": "^3.0.6",
    "redux-saga": "^0.9.3",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "js-cookie": "^2.1.3",
    "moment": "^2.13.0",
    "qs": "^6.2.0"
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    },
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
