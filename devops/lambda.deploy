#!/bin/zsh

mkdir tmp
cp -r ../config.json ../index.js ../lib ../node_modules ../package.json ./tmp
cd ./tmp
zip -r addOneComment .

aws lambda create-function --function-name addOneComment --runtime nodejs4.3 --role arn:aws:iam::309968169905:role/service-role/basic-exec --handler index.handler --zip-file fileb://addOneComment.zip

cd .. && rm -rf ./tmp
